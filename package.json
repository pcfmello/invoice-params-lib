{
  "name": "@gew-tecnologia/invoice-params-lib",
  "version": "1.0.1",
  "description": "Biblioteca que disponibiliza a maioria das parametrizações para geração de NF-e e/ou NFC-e",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist && yarn tsc --build",
    "clean": "tsc --build --clean",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcfmello/invoice-params-lib.git"
  },
  "author": "GEW Tecnologia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pcfmello/invoice-params-lib/issues"
  },
  "homepage": "https://github.com/pcfmello/invoice-params-lib#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
